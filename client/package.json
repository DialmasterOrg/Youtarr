{
  "name": "client",
  "version": "1.57.1",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:3011",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@mui/x-date-pickers": "^5.0.20",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-transition-group": "^4.4.12",
    "axios": "^1.12.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-swipeable": "^7.0.1",
    "react-transition-group": "^4.4.5",
    "typescript": "^4.9.5",
    "webpack": "^5.82.0"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "concurrently -n UNIT,STORY \"npm run test:unit\" \"npm run test:storybook\"",
    "test:unit": "jest --config jest.config.cjs",
    "test:storybook": "npm run test-storybook:ci",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:watch": "jest --config jest.config.cjs --watch",
    "storybook": "storybook dev -p ${STORYBOOK_PORT:-6006}",
    "build-storybook": "storybook build",
    "storybook:ci": "storybook dev -p ${STORYBOOK_PORT:-6006} --ci --quiet",
    "storybook:serve": "http-server storybook-static -p ${STORYBOOK_PORT:-6006} -c-1 --silent",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "npm run build-storybook && concurrently -k -s first -n STORYBOOK,TEST \"npm run storybook:serve\" \"wait-on --timeout 120000 http://127.0.0.1:${STORYBOOK_PORT:-6006}/iframe.html http://127.0.0.1:${STORYBOOK_PORT:-6006}/index.json && npm run test-storybook\"",
    "test-storybook:test-runner": "test-storybook --url http://127.0.0.1:${STORYBOOK_PORT:-6006}",
    "test-storybook:test-runner:ci": "npm run build-storybook && concurrently -k -s first -n STORYBOOK,TEST \"npm run storybook:serve\" \"wait-on --timeout 120000 http://127.0.0.1:${STORYBOOK_PORT:-6006}/iframe.html http://127.0.0.1:${STORYBOOK_PORT:-6006}/index.json && npm run test-storybook:test-runner\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/expect": "^28.1.3-5",
    "@storybook/react": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@storybook/test": "file:packages/storybook-test",
    "@storybook/test-runner": "^0.24.2",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^27.0.1",
    "msw": "^2.12.7",
    "msw-storybook-addon": "^2.0.6",
    "playwright": "^1.51.1",
    "storybook": "^10.1.11",
    "undici": "^7.18.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "wait-on": "^8.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "overrides": {
    "jsdom": {
      "form-data": "^3.0.4"
    }
  }
}
