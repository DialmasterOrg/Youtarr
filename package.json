{
  "name": "youtarr",
  "version": "1.27.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/server.js",
    "start:dev": "nodemon server/server.js",
    "client": "cd client && npm start",
    "start:db": "bash ./scripts/start-dev-dbonly.sh",
    "dev": "concurrently --kill-others \"npm run start:dev\" \"npm run client\"",
    "lint": "eslint ./client/src/. --ext .ts,.tsx && eslint ./server/. --ext .js",
    "lint:frontend": "eslint ./client/src/. --ext .ts,.tsx",
    "lint:backend": "eslint ./server/. --ext .js",
    "lint:fix": "eslint ./client/src/. --ext .ts,.tsx --fix && eslint ./server/. --ext .js --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --config jest.config.js",
    "test:frontend": "cd client && npm test -- --watchAll=false",
    "test:coverage": "jest --config jest.config.js --coverage && cd client && npm test -- --coverage --watchAll=false",
    "test:watch": "jest --config jest.config.js --watch"
  },
  "lint-staged": {
    "./client/src/*.{ts,tsx},./server/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "concurrently": "^6.5.1",
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^30.1.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.15",
    "supertest": "^7.1.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.0",
    "bcrypt": "^6.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.1.1",
    "multer": "^2.0.2",
    "mysql2": "^3.9.4",
    "node-cron": "^3.0.2",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "umzug": "^2.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  }
}
